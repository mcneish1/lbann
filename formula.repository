\form#0:$\hat{y}$
\form#1:$y$
\form#2:\[ \nabla_{\hat{y}} CE(\hat{y}, y) = -y/\hat{y} + (1-y)/(1-\hat{y}) \]
\form#3:\[ CE(y,\hat{y}) = - \sum\limits_{i} \hat{y}_i \log y_i \]
\form#4:\[ \nabla_y CE (y,\hat{y}) = - \hat{y} . / y \]
\form#5:\[ Geom_nll(\hat{y},y) = -y \log(1-\hat{y}) + \log(\hat{y}) \]
\form#6:\[ \nabla_{\hat{y}} \text{Geom}_{\text{nll}} (\hat{y},y) = y/(1-\hat{y}) + 1/\hat{y} \]
\form#7:\[ MAD(\hat{y}, y) = \frac{1}{n} \lVert y - \hat{y} \rVert_1 \]
\form#8:\[ MSE(\hat{y}, y) = \frac{1}{n} \lVert y - \hat{y} \rVert_2^2 \]
\form#9:\[ \nabla_y MSE (y,\hat{y}) = \frac{2}{n} (y - \hat{y}) \]
\form#10:\[ Pois_nll(\hat{y}, y) = \hat{y} - y\log(\hat{y}) + \log(\Gamma(y + 1)) \]
\form#11:\[ \nabla_y Pois_nll(\hat{y},y) = 1 - y/\hat{y} \]
\form#12:$(\alpha_1,\dots,\alpha_K)$
\form#13:$(k_1,\dots,k_K)$
\form#14:$n = \sum_{i=0}^K k_i$
\form#15:\[ Poly_nll(\vec{\alpha}, \vec{k}) = -\log \Gamma(\sum_{i=0}^K \hat{y}_i) + \log \Gamma(n + \sum_{i=0}^K \hat{y}_i) - \sum_{i=0}^K \left\{\log \Gamma(\hat{y}_i + \alpha_i) - \log \Gamma(\alpha_i) \right\} \]
\form#16:\[ \nabla_{alpha_i} Poly_nll(\vec{\alpha}, \vec{k}) = -\Psi(\sum_{i=1}^K \alpha_i) + \Psi(n + \sum_{i=1}^K \alpha_i) - \Psi(k_i + \alpha_i) + \Psi(\alpha_i) \]
\form#17:$ 1 / (1 - p) $
